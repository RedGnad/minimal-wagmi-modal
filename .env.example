# Example environment variables for Sherlock
# Copy this file to .env and fill in the values.

# RPC URL for Monad Testnet (Alchemy, Ankr, etc.)
# e.g. https://monad-testnet.g.alchemy.com/v2/YOUR_API_KEY
VITE_MONAD_RPC_URL="https://monad-testnet.g.alchemy.com/v2/CHANGE_ME"

# SIWE expected domain (used on server verify); set to your deploy domain or dev host:port
VITE_SIWE_DOMAIN="localhost:5173"

# SIWE nonce expiration in minutes
VITE_SIWE_EXP_MINUTES="5"
VITE_SIWE_REQUIRE_SERVER="false" # passer à true quand le backend de vérification est prêt
